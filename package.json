{
  "name": "FindPhotosOfMe",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @FindPhotosOfMe/backend dev",
    "dev:setup": "turbo -F @FindPhotosOfMe/backend dev:setup",
    "dev:telegram": "turbo -F @FindPhotosOfMe/telegram dev",
    "start:telegram": "turbo -F @FindPhotosOfMe/telegram start",
    "build:telegram": "turbo -F @FindPhotosOfMe/telegram build"
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "wrangler": "^4.42.0"
  },
  "packageManager": "pnpm@10.12.4"
}
