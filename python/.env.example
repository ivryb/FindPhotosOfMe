# Cloudflare R2 Configuration
# Set USE_R2=true to enable R2 storage, or USE_R2=false to use local filesystem
USE_R2=true

# R2 Account Credentials
# Get these from your Cloudflare dashboard: https://dash.cloudflare.com/
R2_ACCOUNT_ID=your_account_id_here
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here
R2_BUCKET_NAME=your_bucket_name_here

# R2 Storage Paths
# Input prefix: where to read photos from in R2
R2_INPUT_PREFIX=input

# Output prefix: where to save filtered photos in R2
R2_OUTPUT_PREFIX=output

# Reference photo key in R2 (the photo to match against)
R2_REFERENCE_PHOTO=ref.jpg
